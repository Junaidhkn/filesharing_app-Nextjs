// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model file {
  id          String @id @default(uuid())
  filename    String
  path        String
  size        Int
  emailFrom   String
  emailTo     String
  title       String
  description String

  createdAt DateTime @default(now())
}
